{
  "title": "Experiment Monitoring",
  "tags": ["experiments", "ml"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Experiments Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(experiments_total[5m])",
          "legendFormat": "{{adapter_type}} - {{status}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "title": "Experiment Duration (95th percentile)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(experiment_duration_seconds_bucket[5m]))",
          "legendFormat": "{{adapter_type}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Experiment Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(experiments_total{status=\"success\"}[5m])) / sum(rate(experiments_total[5m])) * 100",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 80, "color": "yellow"},
              {"value": 95, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Experiments",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "expr": "sum(active_experiments)",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Experiments by Adapter Type",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "sum by (adapter_type) (experiments_total)",
          "legendFormat": "{{adapter_type}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        }
      }
    },
    {
      "id": 6,
      "title": "Experiment Failures by Error Type",
      "type": "barchart",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum by (error_type) (experiment_failures_total)",
          "legendFormat": "{{error_type}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "uid": "experiment-monitoring",
  "editable": true
}
