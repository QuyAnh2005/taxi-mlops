{
  "title": "Prefect Flow Monitoring",
  "tags": ["prefect", "workflows"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Flow Runs Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(prefect_flow_runs_total[5m])",
          "legendFormat": "{{flow_name}} - {{status}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "title": "Flow Run Duration (95th percentile)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(prefect_flow_run_duration_seconds_bucket[5m]))",
          "legendFormat": "{{flow_name}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Flow Run Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(prefect_flow_runs_total{status=\"success\"}[5m])) / sum(rate(prefect_flow_runs_total[5m])) * 100",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 80, "color": "yellow"},
              {"value": 95, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Flows",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "expr": "prefect_active_flows",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Flow Runs by Status",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "sum by (status) (prefect_flow_runs_total)",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        }
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "uid": "prefect-monitoring",
  "editable": true
}
